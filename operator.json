  "description": "Interactive heatmap operator for Tercen",
  "tags": [
    "data visualisation",
    "plot"
  ],
  "authors": [
    "rdw"
  ],
  "urls": [
    "https://github.com/tercen/heatmap_dashboard_operator"
  ],
  "container": "tercen/heatmap_dashboard_operator:0.3.0",
  "isWebApp": true,
  "isViewOnly": false,
  "properties": [
    {
      "kind": "DoubleProperty",
