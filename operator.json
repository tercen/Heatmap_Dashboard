{
  "name": "Heatmap Dashboard",
  "description": "Interactive heatmap operator for Tercen",
  "tags": ["heatmap", "pamgene"],
  "authors": ["rdw"],
  "urls": ["https://github.com/tercen/Heatmap_Dashboard"],
  "container": "tercen/heatmap_dashboard:latest",
  "isWebApp": true,
  "isViewOnly": false,
  "properties": [

    {
    "kind": "DoubleProperty",
    "name": "qMin",
    "defaultValue": 0.01,
    "description": "Quantile for default lower scale limit"
    },
    {
    "kind": "DoubleProperty",
    "name": "qMax",
    "defaultValue": 0.99,
    "description": "Quantile for default upper scale limit"
    }  
  ]
}
